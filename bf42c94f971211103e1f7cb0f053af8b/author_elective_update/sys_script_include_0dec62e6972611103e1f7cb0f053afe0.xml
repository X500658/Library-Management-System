        <description>generate start and due dates for book transactions</description>
        <name>BookDates</name>
        <script><![CDATA[var BookDates = Class.create();
BookDates.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
    getStartDate: function() {
		return new GlideDateTime();
	},
	getDueDate: function() {
		var gdt= new GlideDateTime();
		gdt.addDaysLocalTime(7);
		return gdt;
	},
    type: 'BookDates'
});]]></script>
